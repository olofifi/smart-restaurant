<% if(session.msg){%> 
    <% if(session.msgType=='success'){%> 
      <div class="alert alert-success" role="alert">
      <%= session.msg %>
      </div>
      <% session.msg = null;%>
      <% }%> 
      <% if(session.msgType=='error'){%> 
      <div class="alert alert-danger" role="alert">
        <%= session.msg %>
      </div>
      <% session.msg = null;%>
      <% }%> 
      <% if(session.msgType=='info'){%> 
        <div class="alert alert-warning" role="alert">
          <%= session.msg %>
        </div>
        <% session.msg = null;%>
        <% }%> 
    <% }%> 